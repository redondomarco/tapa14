sudo apt-get install build-essential libpoppler-cpp-dev pkg-config python3-dev python3-pip swig libssl-dev git unzip ipython3 python3-tk 

sudo pip3 install -r requirements.txt

#base de datos
sudo apt install postgresql postgresql-server-dev-11 libpq-dev

echo # configurar base de datos
echo sudo passwd postgres
echo su - postgres createuser -PE -s t14user
echo createdb -O t14user -E UTF8 tapa14v1

cd ~/web2py/applications/tapa14
git clone https://github.com/redondomarco/tapa14-files.git files
cp ~/web2py/examples/logging.example.conf ~/web2py/logging.conf

